---
title: Partners
page_title: Partners
---

<section class="page inner-page partners">
	<!-- page title -->

	<div class="inner-page-header" id="subheader">
		<div class="container">
			<div class="row">
				<h1 class="headline">Partners</h1>
			</div>
		</div>
	</div>
	<!-- page contents -->

	<%
		 myPartnersJSON = File.read("source/sipart.json");

		 myPartners = JSON.parse(myPartnersJSON)
		 %>

	<article class="intropart">
		<div class="container">
			<div class="row">
				<div class="col-md-6 col-sm-12">
					<h2>Technology Partners</h2>
					<p> Our strategic <a href="#techparts">Technology Partners</a> are public
						cloud companies and fundamental technology providers with whom we've
						aligned. We are honored to build the future of massively scalable
						application delivery with them.
					</p>
				</div>
				<div class="col-md-6 col-sm-12">
					<h2>System Integration Partners</h2>
					<p> Our <a href="#sipart">System Integration Partners</a> program recognizes
						companies who are leaders, practitioners, and educators of our
						workflow and techniques. They have each demonstrated a deep command of
						our toolset.  They are our trusted partners in teaching and
						implementing the HashiCorp way.  SI Partners are found around the
						world.
					</p>
				</div>
			</div><!-- row -->
		</div><!-- container -->
	</article><!-- partners -->

	<article class="techparts" id="techparts">
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
					<h1>
						Technology Partners
					</h1>
				</div>
			</div> <!-- row -->
			<div class="row">
				<div class="col-xs-12">
					<ul class="techpart-logos unstyled">
						<li class="tpart tp1"><a href="http://www.aws-partner-directory.com/PartnerDirectory/PartnerDetail?Name=HashiCorp"><i></i></a></li>
						<li class="tpart tp2"><a href="https://cloud.google.com"><i></i></a></li>
						<li class="tpart tp3"><a href="https://azure.microsoft.com"><i></i></a></li>
						<li class="tpart tp4"><a href="http://www.cisco.com"><i></i></a></li>
						<li class="tpart tp5"><a href="http://www.vmware.com"><i></i></a></li>
						<li class="tpart tp6"><a href="https://www.digitalocean.com/"><i></i></a></li>
					</ul> <!-- techpart logos -->
				</div> <!-- row -->
		</div> <!-- container -->
	</article> <!-- techparts -->

	<article class="partners sipart" id="sipart">
		<div class="container">
			<div class="row">
				<div class="si-content col-xs-12">
					<h1> System Integration Partners </h1>
				</div>
			</div> <!-- row -->
			<div class="row">
        <div class="panel-wrap">
				<% myPartners.each_with_index { |val, index| %>

					<div class="pan">
						<div class="pan-heading">
							<h2><a href=<%= val["url"] %>><%= val["name"] %></a></h2>
						</div> <!-- panel-heading -->
						<div class="pan-body">
							<a href=<%= val["url"] %>><div class="plogo">
									<i class=<%= val["logo"] %> ></i>
							</div></a>
							<p class="ptagline"> <%= val["tagline"] %> </p>
							<p class="plocations"> <i> <%= val["locations"] %> </i> </p>
							<p class="pldesc"> <%= val["ldesc"] %> </p>
							<% if val["contact"] != "" %>
							<div class="atbottom"><a class="hbtn blue plain careted contactus" href="mailto:<%= val["contact"] %>?subject=HashiCorp Partner request&body=Hello,%0a%0dI'm interested in your HashiCorp expertise.%0a%0dPlease contact me.%0a%0d%0a%0dThank you.%0a%0d%0a%0dBest Regards,"> contact <%= val["pretty"] %> <span class="hcaret blue"></span></a></div>
							<% elsif val["contact"] == "" %>
							<a class="hbtn blue plain careted contactus">(missing email)<span class="hcaret blue"></span></a>
							<% end %>
						</div> <!-- panel-body -->
					</div>	   <!-- panel -->

				<% } %>

        </div> <!-- panel-wrap -->
			</div> <!-- row -->
		</div> <!-- container -->
	</article> <!-- sipart -->

  <article>
    <div class="bnr light-bnr">
    	<div class="container">
    		<div class="row">
    			<div class="col">
    				<h4>If your organization is interested in
              becoming a HashiCorp Partner, we welcome you to
              learn more. Send us an email.</h4>
    				<div class="bnr-btn">
    				  <a class="hbtn orange lrg careted" href="mailto:partner-request@hashicorp.com?subject=Interested in HashiCorp Partner Program&body=I'd like to learn more about the HashiCorp Partner Program.%0a%0DThank you.">Email Us<span class="hcaret"></span></a>
    				</div>
    			</div>
    		</div>
    	</div>
    </div>
  </article> <!-- /#home-cta -->
</section>
